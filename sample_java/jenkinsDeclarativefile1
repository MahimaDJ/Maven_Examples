pipeline {
    agent any

    environment {
        
        PATH="/usr/share/maven/bin:$PATH"
    }
    stages {
        stage('Github checkout') {
            steps {
                echo " github checkout"
                          }
        }
        
        stage ('Maven Build') {
           steps {
               sh  "cd sample_java"
               sh "mvn clean package"
           }
        }
    }
}
